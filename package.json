		"url": "git+ssh://git@github.com:yutak23/vitest-openapi.git"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"license": "MIT",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"scripts": {
		"build": "run-s lint clean tsc",
		"tsc": "tsc -p ./tsconfig.json",
		"clean": "rm -rf ./dist",
		"test": "vitest run --coverage",
		"lint": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts",
		"lint:fix": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts --fix",
		"format": "prettier -c .",
