{
	"name": "vitest-openapi",
	"version": "1.0.0",
	"author": "yutak23 <yuta.katayama.23+github@gmail.com> (https://github.com/yutak23)",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:yutak23/vitest-openapi.git"
	},
	"license": "MIT",
	"type": "module",
	"types": "./esm/index.d.ts",
	"main": "./cjs/index.js",
	"module": "./esm/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./esm/index.d.ts",
				"default": "./esm/index.js"
			},
			"require": {
				"types": "./cjs/index.d.ts",
				"default": "./cjs/index.js"
			},
			"default": "./cjs/index.js"
		}
	},
	"scripts": {
		"build": "run-s clean build:*",
		"build:esm": "tsc -p ./tsconfig.esm.json && echo '{\"type\":\"module\"}' > esm/package.json",
		"build:cjs": "tsc -p ./tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
		"clean": "rm -rf ./esm ./cjs",
		"test": "vitest run --coverage",
		"lint": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts",
		"lint:fix": "eslint . --ext .cjs,.mjs,.js,.ts,.cts,.mts --fix",
		"format": "prettier -c .",
		"format:fix": "prettier --write .",
		"ncu": "ncu"
	},
	"dependencies": {},
	"devDependencies": {
		"@tsconfig/node16": "^16.1.1",
		"@tsconfig/recommended": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^6.7.3",
		"@typescript-eslint/parser": "^6.7.3",
		"@vitest/coverage-v8": "^0.34.5",
		"eslint": "^8.50.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"npm-check-updates": "^16.14.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2",
		"vitest": "^0.34.5"
	},
	"files": [
		"esm",
		"cjs"
	],
	"keywords": [
		"vitest",
		"openapi",
		"testing",
		"response",
		"validate",
		"assertions"
	]
}
